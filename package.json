{
  "name": "gallery-manager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "predeploy": "npm run build",
    "migrate:r2": "tsx --env-file=.env.development scripts/migrate-imgur-to-r2.ts",
    "migrate:r2:all": "tsx --env-file=.env.development scripts/migrate-imgur-to-r2.ts --all",
    "diagnose-album": "tsx --env-file=.env.development scripts/diagnose-album.ts",
    "repair-album": "tsx --env-file=.env.development scripts/repair-album.ts",
    "rebuild-album": "tsx --env-file=.env.development scripts/rebuild-album-metadata.ts",
    "check-album": "tsx --env-file=.env.development scripts/check-album-exists.ts",
    "cleanup-orphans": "tsx --env-file=.env.development scripts/cleanup-orphaned-albums.ts",
    "inspect-orphans": "tsx --env-file=.env.development scripts/inspect-orphaned-albums.ts",
    "delete-album": "tsx --env-file=.env.development scripts/delete-album.ts",
    "rebuild-index": "tsx --env-file=.env.development scripts/rebuild-index.ts"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.4.0",
    "@auth0/auth0-spa-js": "^2.3.0",
    "@aws-sdk/client-s3": "^3.958.0",
    "@aws-sdk/s3-request-presigner": "^3.958.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@types/react-dropzone": "^4.2.2",
    "axios": "^1.9.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.514.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/jszip": "^3.4.0",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}
